(const dim      [window.innerWidth, window.innerHeight];
)
(const size 25)
(set module.exports
     'size size
     'angle-zoom 1
     'noise-z 1
     'field-force 100
     'dimensions dim
     'columns (Math.ceil (/ (first dim) size))
     'rows (Math.ceil (/ (second dim) size))
     'decay 0.04
     'maxLength 4
     'ant-limit 200
     'max-in-decay 50
     'limit-decay false
     'ant-influence 0.03
     'friction 8
     'spawn-rate 100
     )
