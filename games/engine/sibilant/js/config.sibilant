(const dim      [window.innerWidth, window.innerHeight];
)
(const size 10)
(set module.exports
     'size size
     'angle-zoom 1000
     'noise-z 1000
     'field-force 100000
     'dimensions dim
     'columns (Math.ceil (/ (first dim) size))
     'rows (Math.ceil (/ (second dim) size))
     'decay 0.001
     'maxLength 7
     'max-in-decay 50
     'ant-influence 0.1
     'friction 16
     )
