(const dim      [window.innerWidth, window.innerHeight];
)
(const size 10)
(set module.exports
     'size size
     'angle-zoom 100
     'noise-z 100
     'field-force 1000
     'dimensions dim
     'columns (Math.ceil (/ (first dim) size))
     'rows (Math.ceil (/ (second dim) size))
     'decay 0.001
     'friction 16
     )
