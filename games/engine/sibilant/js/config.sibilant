(const dim      [window.innerWidth, window.innerHeight];
)
(const size 1)
(set module.exports
     'size size
     'angle-zoom 30
     'noise-z 70
     'field-force 1000
     'dimensions dim
     'columns (Math.ceil (/ (first dim) size))
     'rows (Math.ceil (/ (second dim) size))
     'decay 10.1
     'maxLength 100.5
     'ant-limit 200
     'max-in-decay 400
     'max-trail 400
     'min-trail 100
     'decay-on-collision true
     'limit-decay true
     'ant-influence 1.8
     'friction  2.5
     'collision-static 1
     'spawn-static 1
     'spawn-rate 1000
     )
