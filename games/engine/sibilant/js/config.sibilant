(const dim      [window.innerWidth, window.innerHeight];
)
(const size 10)
(set module.exports
     'size size
     'angle-zoom 10
     'noise-z 1
     'field-force 1000
     'dimensions dim
     'columns (Math.ceil (/ (first dim) size))
     'rows (Math.ceil (/ (second dim) size))
     'decay 0.9
     'friction 32
     )
